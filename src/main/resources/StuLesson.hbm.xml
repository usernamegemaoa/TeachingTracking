<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bj.po.StuLesson" table="d_stu_lesson" schema="dbteaching" lazy="false">
        <id name="stleId">
            <column name="stle_id" sql-type="int(11)"/>
        </id>
        <property name="stuId">
            <column name="stu_id" sql-type="int(11)"/>
        </property>
        <property name="lessonId">
            <column name="lesson_id" sql-type="int(11)"/>
        </property>
        <property name="stleWork">
            <column name="stle_work" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>