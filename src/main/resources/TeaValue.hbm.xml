<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bj.po.TeaValue" table="d_tea_value" schema="dbteaching" lazy="false">
        <id name="valueId">
            <column name="value_id" sql-type="int(11)"/>
        </id>
        <property name="stuId">
            <column name="stu_id" sql-type="int(11)"/>
        </property>
        <property name="teaId">
            <column name="tea_id" sql-type="int(11)"/>
        </property>
        <property name="value1">
            <column name="value_1" sql-type="int(11)"/>
        </property>
        <property name="value2">
            <column name="value_2" sql-type="int(11)"/>
        </property>
        <property name="value3">
            <column name="value_3" sql-type="int(11)"/>
        </property>
        <property name="valueContent">
            <column name="value_content" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>