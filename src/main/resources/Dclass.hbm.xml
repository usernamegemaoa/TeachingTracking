<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bj.po.Dclass" table="d_class" schema="dbteaching" lazy="false">
        <id name="classId">
            <column name="class_id" sql-type="int(11)"/>
        </id>
        <property name="classInyear">
            <column name="class_inyear" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="subjectId">
            <column name="subject_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="classNum">
            <column name="class_num" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>