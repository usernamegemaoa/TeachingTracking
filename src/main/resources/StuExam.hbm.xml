<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bj.po.StuExam" table="d_stu_exam" schema="dbteaching" lazy="false">
        <id name="stexId">
            <column name="stex_id" sql-type="int(11)"/>
        </id>
        <property name="stuId">
            <column name="stu_id" sql-type="int(11)"/>
        </property>
        <property name="questionId">
            <column name="question_id" sql-type="int(11)"/>
        </property>
        <property name="stexMark">
            <column name="stex_mark" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>